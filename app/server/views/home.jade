
extends layout

block content
	div.navbar.navbar-fixed-top
		div.navbar-inner
			div.container-fluid
				ul#global-nav.nav.pull-left
					a(href='#').brand Control Panel
				button#btn-logout.btn.btn-primary
					i.icon-lock.icon-white
					| Sign Out

	.github-fork-ribbon-wrapper.right(style='top:40px;')
		.github-fork-ribbon(style='background-color: #333;')
			a(href="https://github.com/kuu/ConcurrentStreamLimits") View On Github


	include player

block scripts
	script(src='//player.ooyala.com/v3/7267438d470c471eb75a165a8b670617')
	script(src='/vendor/jquery.min.js')
	script(src='/vendor/jquery.form.js')
	script(src='/vendor/bootstrap-modal.js')
	script(src='/vendor/bootstrap-transition.js')
	script(type='text/javascript').
		var playerToken = '!{token}';
		var embedCode = '!{embedCode}';  
	script(src='/js/views/home.js')
	script(src='/js/controllers/homeController.js')
	script(src='/js/form-validators/accountValidator.js')
